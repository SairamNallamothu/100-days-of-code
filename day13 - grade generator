testName = input("Enter test name? ")
maxScore = int(input("Enter max score? "))
marks = int(input("Enter your marks you recieved? "))


percentage = float((marks/maxScore) * 100)
percentage = round(percentage, 2)

if(percentage >= 90):
  grade = "A+"
elif (percentage >= 80 and percentage < 90 ):
    grade = "A"
elif (percentage >= 70 and percentage < 80 ):
    grade = "B"
elif (percentage >= 60 and percentage < 70 ):
    grade = "C"
elif (percentage >= 50 and percentage < 60 ):
  grade = "D"
else:
  grade = "U"

print("You got a ",grade," in ",testName, " with a percentage of ", percentage)
  
